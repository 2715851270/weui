  background-position: center;
  background-size: 46rpx;
  border-style: dashed;
}

.form-action {
  padding: 30rpx;
  padding-top: 10rpx;
}

.form-before {
  border-right: 1rpx solid #f4f4f6;
  color: #1a1a1a;
}

