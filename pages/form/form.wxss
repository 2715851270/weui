.form-messages {
  position: fixed;
  top: 0;
  display: block;
  width: 100%;
  background-color: rgba(233, 79, 79, 0.85);
  padding: 10rpx 40rpx;
  font-size: 24rpx;
  color: #fff;
  z-index: 99;
}

.form-agreement {
  font-size: 26rpx;
  color: #909399;
  padding: 0 40rpx;
  margin-bottom: 20rpx;
}

.form-agreement checkbox .wx-checkbox-input {
  height: 26rpx;
  width: 26rpx;
  position: relative;
  top: -2rpx;
  margin-right: 0;
}

.form-agreement checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
  font-size: 26rpx;
}

.form-agreement navigator {
  display: inline-block;
}

.form-group.error .form-control {
  color: #e64340
}
.form-group.error::after {
  content: '';
  position: absolute;
  left: calc(100% - 60rpx);
  top: 25rpx;
  width: 40rpx;
  height: 40rpx;
  background-color: transparent;
  background-image: url("data:image/svg+xml,%3Csvg t='1545792486198' class='icon' style='' viewBox='0 0 1024 1024' version='1.1' xmlns='http://www.w3.org/2000/svg' p-id='2549' xmlns:xlink='http://www.w3.org/1999/xlink' width='200' height='200'%3E%3Cdefs%3E%3Cstyle type='text/css'%3E%3C/style%3E%3C/defs%3E%3Cpath d='M512 1024C229.227 1024 0 794.773 0 512S229.227 0 512 0s512 229.227 512 512-229.227 512-512 512z m0-372.373a69.813 69.813 0 1 0 0 139.648 69.813 69.813 0 0 0 0-139.638z m0-418.902a59.307 59.307 0 0 0-59.243 61.995l10.582 232.843a48.704 48.704 0 0 0 97.322 0l10.582-232.843A59.307 59.307 0 0 0 512 232.725z' p-id='2550' fill='%23e64340'%3E%3C/path%3E%3C/svg%3E");
